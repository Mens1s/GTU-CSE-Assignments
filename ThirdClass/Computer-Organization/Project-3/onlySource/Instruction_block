v0x126719a20 .array "instruction_memory", 31 0, 31 0;
v0x126719ac0_0 .net "pc", 31 0, v0x126719c50_0;  1 drivers
E_0x126706e40 .event posedge, v0x126719ac0_0;
    .scope S_0x1267094c0;
T_0 ;
    %vpi_call 3 9 "$readmemb", "instructions.mem", v0x126719a20 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x1267094c0;
T_1 ;
    %wait E_0x126706e40;
    %load/vec4 v0x126719ac0_0;
    %ix/load 5, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 5;
    %ix/vec4 4;
    %load/vec4a v0x126719a20, 4;
    %store/vec4 v0x1267064e0_0, 0, 32;
    %jmp T_1;
    .thread T_1;
    .scope S_0x126706370;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x126719c50_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 22 "$display", "pc = %b, instruction = %b", v0x126719c50_0, v0x126719b90_0 {0 0 0};
    %pushi/vec4 4, 0, 32;
